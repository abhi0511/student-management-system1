@model List< student_data_management.Models.Domain.Student>

@{
}
<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>roll_no</th>
            <th>Email</th>
            <th>Address</th>
            <th>dob</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
          <tr>

              <td>@student.Id</td>
              <td>@student.Name</td>
                <td>@student.rollno</td>
                <td>@student.Email</td>
                <td>@student.address</td>
                <td>@student.dob.ToString("dd-mm-yyyy")</td>
                <td><a href="Student/view/@student.Id">view</a></td>
          </tr>  

        }
    </tbody>
</table>